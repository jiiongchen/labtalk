range rr2 = [Book1]exp!col(1); // 定义目标范围
rr2 = rr1; // 复制数据

%z = "ln([Book1]Sheet1!3)";
wks.col2.SetFormula(z);

fitLR (1,2); 

[Book1]exp!cell(1,2)=1/fitlr.b;
