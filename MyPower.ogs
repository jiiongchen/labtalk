newsheet name:=power col:=3;
page.active= "power";

%z = "ln([Book1A]Sheet1!1)";
wks.col1.SetFormula(z);
%z = "ln([Book1A]Sheet1!3)";
wks.col2.SetFormula(z);

fitLR (1,2); 

%f = "exp(fitlr.a+fitlr.b*(col(1)))";
wks.col3.SetFormula(f);

[Book1A]power!cell(1,2)=fitlr.b;
[Book1A]power!cell(2,2)=fitlr.berr;

range rr1 = [Book1A]Sheet1!col(1); // 定义源范围
range rr2 = [Book1A]power!col(1); // 定义目标范围
rr2 = rr1; // 复制数据

plotxy iy:=Sheet1!(1,2) plot:=202 color:=color(0,0,0);
plotxy iy:=Sheet1!(1,3) plot:=201 ogl:=1;
plotxy iy:=[Book1A]power!(1,3) plot:=200 ogl:=1;